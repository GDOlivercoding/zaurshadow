class Repeat {
    init(value, count) {
        this.value = value;
        this.count = count;
        this.index = 1;
    }

    iter() {
        return this;
    }

    next() {
        if this.index > this.count {
            return nil;
        }

        this.index += 1;
        return this.value;
    }
}

var iterable = Repeat("meow", 5);

for iteration of iterable {
    print iteration;
}